<%= provide(:title, "Access Group")%>
<h1>Add a location to this group.</h1>

<%= form_with(url: "/access_groups/#{@access_group.id}/locations", local: true) do |f|%>
  <%= f.label :location_id%>
  <%= f.collection_select :location_id, @current_user.locations,:id,:name%>

  <%= f.submit "Submit", class: "btn btn-primary"%>
<% end%>