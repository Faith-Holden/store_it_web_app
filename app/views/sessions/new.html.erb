<% provide(:title, "Log In")%>
<h1> Log In </h1>


<%= form_with(url: login_path, scope: :session, local: true) do |f|%>
  <%= f.label :email %>
  <%= f.text_field :email, class: 'form-control'%>

  <%= f.label :password%>
  <%= f.password_field :password%>

  <%= f.submit "Log In", class: "btn btn-primary"%>
<% end%>